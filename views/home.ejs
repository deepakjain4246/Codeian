<p>
    <h1>Codeian <%=title%></h1>
</p>

<section id="feed-post">
    <h1>Posts</h1>
    <%if(locals.user){%>
    <form action="/posts/create" id="new-post-form" method="POST">
        <textarea name="content" cols="30" rows="3" >Type here..
        </textarea>
        <input type="submit" value="Post">
    </form>
   <%}%>
</section>

<section id="post-list-container">
    <ul>
        <% for(post of posts){ %>
       

            <%-include('_post')-%>

        <%}%>
    </ul>
</section>

<section id="user-friends">
    <h4>Friends</h4>
    <% for(u of all_users){%>
      <p>
          <a href="/users/profile/<%=u.id%>"><%=u.name%></a>
      </p>
    <%}%>
</section>